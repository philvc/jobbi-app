{
	"name": "nextjs-starter",
	"version": "1.0.0",
	"private": false,
	"author": {
		"email": "maxime@nightborn.be",
		"name": "Denuit Maxime",
		"url": "https://nightborn.be"
	},
	"keywords": [
		"react",
		"nextjs",
		"chakra-ui",
		"storybook",
		"jest"
	],
	"scripts": {
		"dev": "next dev",
		"build": "cross-env NODE_ENV=production next build",
		"export": "next export",
		"start": "next start",
		"generate-api": "orval --config ./orval.config.js",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"dependencies": {
		"@capacitor/android": "^3.2.5",
		"@capacitor/cli": "^3.2.5",
		"@capacitor/core": "^3.2.5",
		"@capacitor/ios": "^3.2.5",
		"@chakra-ui/react": "^1.6.8",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@fontsource/poppins": "^4.5.0",
		"@storybook/addon-controls": "^6.3.8",
		"@storybook/react": "^6.3.12",
		"@supabase/supabase-js": "^1.25.2",
		"axios": "^0.21.1",
		"cross-env": "^7.0.3",
		"faker": "^5.5.3",
		"formik": "^2.2.9",
		"framer-motion": "^4.1.17",
		"i": "^0.3.7",
		"lodash": "^4.17.21",
		"msw": "^0.35.0",
		"next": "12.0.2",
		"npm": "^8.1.1",
		"orval": "^6.2.3",
		"path": "^0.12.7",
		"react": "17.0.2",
		"react-color": "^2.19.3",
		"react-dom": "17.0.2",
		"react-dropzone": "^11.4.2",
		"react-i18next": "^11.11.4",
		"react-loading-skeleton": "^2.2.0",
		"react-lottie": "^1.2.3",
		"react-modal": "^3.14.3",
		"react-query": "^3.13.4",
		"react-select": "^4.3.1",
		"react-textarea-autosize": "^8.3.2",
		"use-supabase": "^1.0.3"
	},
	"devDependencies": {
		"@types/node": "16.11.6",
		"@types/react": "17.0.33",
		"eslint": "7.32.0",
		"eslint-config-next": "12.0.2",
		"typescript": "4.4.4"
	},
	"resolutions": {
		"react": "17.0.1"
	},
	"browserslist": [
		">0.3%",
		"not ie 11",
		"not dead",
		"not op_mini all"
	],
	"jest": {
		"testPathIgnorePatterns": [
			"./.next/",
			"./node_modules/",
			"./.storybook/"
		],
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
		},
		"moduleNameMapper": {
			"\\.svg": "<rootDir>/__mocks__/svgrMock.js"
		}
	}
}
